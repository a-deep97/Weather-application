<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="./css/style.css">
    <title>Know your Weather</title>
</head>
<body>
    <header>hello this is header</header>
    <section id="headline">
        <div id="intro-container">Weather</div>
        <div id="search-container"><%- include('./_searchbox') %> </div>
    </section>
    <section id="main-section">
        <div id="main-container">
            <div id="main-info-container">
                <div id="name-date-container">
                    <div id="name-container">New Delhi , IN</div>
                    <div id="date-container">wednesday | 28 january</div>
                </div>
                <div id="weather-summary-container">
                    <div id="weather-info-container">
                        <div id="temperature-container">
                            <div id="temperature">24</div>
                            <div id="min-max-temp">16 | 30</div>
                        </div>
                        <div id="weather-description-container">Mostly Sunny</div>
                    </div>
                    <div id="icon-container">
                        <img src="./img/sunny-img.png" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div id="side-container">
            <div id="container-1" class="side-info-container">
                <div id="feels-like" class="side-info">
                    <div id="feels-like-label" class="side-label">Feels like</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-2" class="side-info-container">
                <div id="humidity" class="side-info">
                    <div id="feels-like-label" class="side-label">Humidity</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-3" class="side-info-container">
                <div id="pressure" class="side-info">
                    <div id="feels-like-label" class="side-label">Pressure</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-4" class="side-info-container">
                <div id="wind-speed" class="side-info">
                    <div id="feels-like-label" class="side-label">Wind speed</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-5" class="side-info-container">
                <div id="visibility" class="side-info">
                    <div id="feels-like-label" class="side-label">Visibility</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-6" class="side-info-container">
                <div id="sunrise" class="side-info">
                    <div id="feels-like-label" class="side-label">Sunrise</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
                <div id="sunset" class="side-info">
                    <div id="feels-like-label" class="side-label">Sunset</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
            <div id="container-7" class="side-info-container">
                <div id="timezone" class="side-info">
                    <div id="feels-like-label" class="side-label">Time zone</div>
                    <div id="feels-like-value" class="side-value"></div>
                </div>
            </div>
        </div>    
    </section>
    <footer>hello this is footer</footer>
</body>
</html>